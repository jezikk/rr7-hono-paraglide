    "@react-router/dev": "^7.5.0",
    "@tailwindcss/vite": "^4.1.3",
    "@types/node": "^20",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "react-router-devtools": "^1.1.10",
    "tailwindcss": "^4.1.3",
